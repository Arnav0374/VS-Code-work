import tkinter as tk

root = tk.Tk()
root.title("Irish Flag")

canvas = tk.Canvas(root, width=450, height=300, bg="white")
canvas.pack()

# Draw the three vertical stripes
canvas.create_rectangle(50, 50, 150, 250, fill="#169B62", width=0)   # Green (left)
canvas.create_rectangle(150, 50, 250, 250, fill="white", width=0)    # White (center)
canvas.create_rectangle(250, 50, 350, 250, fill="#FF883E", width=0)  # Orange (right)

root.mainloop()
